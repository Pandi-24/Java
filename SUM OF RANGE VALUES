import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int i = sc.nextInt();
    int j = sc.nextInt();
    double sum =0;
    for(int k=i;k<=j;k++){
        sum+=k;
        
    }
    System.out.println("The sum of Range value from "+i+" to "+j+" is "+sum);
    }
}
