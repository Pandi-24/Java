import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int c = sc.nextInt();
    if(a > b && a > c){
        System.out.println("Number1 is maximum with value of "+(double)a);
    }
    else if(b>a && b>c){
        System.out.println("Number2 is maximum with value of "+(double)b);
    }
    else{
        System.out.println("Number3 is maximum with value of "+(double)c);
    }
    
    }
}
