import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n =sc.nextInt();
        int sum=1;
        if(n<0){
            System.out.println("Invalid Input");
        }
        else{
            for(int k=1;k<=n;k++){
                sum*=k;
            }
            System.out.println("The Factorial of "+n+" is "+sum);
        }
    }
}
